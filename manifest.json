{
    "name": "Bungie Enhancement Suite",
    "version": "0.32",
    "manifest_version": 2,
    "description": "Enhances bungie.net with numerous features.",
    "icons": {
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "http*://*.bungie.net/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://*.bungie.net/*", "https://*.bungie.net/*"
            ],
            "js": [
                "xt.js", "bungie-xt-api.js", "helper.js", "script.user.js"
            ]
        }
    ]
}